# Prettier ls.
alias ll='gls -lhp --color --group-directories-first'

# Where is Java?
export JAVA_HOME=`/usr/libexec/java_home`

# Where is Android?
export ANDROID_HOME='/usr/local/opt/android-sdk'

# Use brew's vim instead of built-in.
alias vim='/usr/local/Cellar/vim/7.4.1190/bin/vim'

# Authenticate Homebrew requests to GitHub
export HOMEBREW_GITHUB_API_TOKEN='cb3c83df0e16dc59fea74d5d22db12209f6d8ad5'
