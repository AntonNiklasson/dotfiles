[user]
	name = Anton Niklasson
	email = niklasson.anton@gmail.com
[github]
	user = AntonNiklasson
[core]
	autocrlf = input
	whitespace = fix,space-before-tab,tab-in-indent,trailing-space
	excludesfile = ~/.gitignore
	editor = vim
[apply]
	whitespace = fix
[alias]
	statsu = status
	psuh = push
	unstage = reset HEAD
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[branch "master"]
	remote = origin
	merge = refs/heads/master
[color]
	ui = auto
	diff = auto
	status = auto
	branch = auto
[help]
	autocorrect = 1
[merge]
	log = true
[rerere]
	enabled = 1
	[url "git@github.com:"]
	insteadOf = "gh:"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
	insteadOf = "github:"
[url "git@gist.github.com:"]
	insteadOf = "gst:"
	pushInsteadOf = "gist:"
	pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
	insteadOf = "gist:"
[push]
	default = current
