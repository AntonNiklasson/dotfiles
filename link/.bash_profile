# SETTINGS
export HISTCONTROL=ignoredups
export PS1="\n\n\e[1;35m\W\e[m $ "
export PATH="/Applications/XAMPP/xamppfiles/bin:$PATH"
export PATH=/usr/local/bin:$PATH


# ALIASES
alias ll='ls -FHhl'
alias la='ll -a'
alias ..='cd ..'
alias ...='..;..'
alias cl='clear'
alias mkdir='mkdir -p'

alias v='vim'
alias vi='vim'

alias g='git'
alias ga='g add'
alias gb='g branch'
alias gba='gb -a'
alias gc='g commit'
alias gcm='gc -m'
alias gco='g checkout'
alias gd='g diff'
alias gdc='gd --cached'
alias gl='g log'
alias gs='g status'

alias art='php artisan'
